{
  "name": "nirvana-restaurant",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev -H 0.0.0.0 -p 3000",
    "build": "cross-env SKIP_DB_CONNECT=1 prisma generate && next build",
    "build:compile": "cross-env SKIP_DB_CONNECT=1 prisma generate && next build --experimental-build-mode compile",
    "build:debug": "cross-env SKIP_DB_CONNECT=1 prisma generate && next build --debug",
    "postbuild": "node scripts/copy-standalone-files.js",
    "start": "next start -H 0.0.0.0 -p 3000",
    "start:standalone": "node .next/standalone/server.js",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.2.0",
    "@react-google-maps/api": "^2.19.3",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "framer-motion": "^12.23.12",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "next": "^15.3.2",
    "nodemailer": "^6.9.0",
    "react": "^19.1.1",
    "react-datepicker": "^8.7.0",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.3",
    "react-intersection-observer": "^9.16.0",
    "recharts": "^3.0.2",
    "stripe": "^18.5.0",
    "tailwindcss-textshadow": "^2.1.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.34.0",
    "postcss": "^8.5.5",
    "prisma": "^6.2.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
